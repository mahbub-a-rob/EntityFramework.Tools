{
  "version": "1.2.0-*",
  "description": "Entity Framework Core Command Line Tools",
  "dependencies": {
    "Microsoft.Extensions.CommandLineUtils.Sources": {
      "version": "1.2.0-*",
      "type": "build"
    }
  },
  "buildOptions": {
    "emitEntryPoint": true,
    "keyFile": "../../tools/Key.snk"
  },
  "configurations": {
    "Debug_x86": {
      "buildOptions": {
        "platform": "anycpu32bitpreferred",
        "define": [
          "DEBUG"
        ],
        "outputName": "ef.x86"
      }
    },
    "Release_x86": {
      "buildOptions": {
        "platform": "anycpu32bitpreferred",
        "outputName": "ef.x86"
      }
    }
  },
  "frameworks": {
    "net451": {},
    "netcoreapp1.0": {
      "dependencies": {
        "Microsoft.NETCore.App": {
          "type": "platform",
          "version": "1.2.0-*"
        }
      }
    }
  }
}